{
  "name": "InternetArchive", 
  "active": false, 
  "default": false, 
  "connector": "RequestsGet", 
  "url": "https://archive.org/services/search/v1/scrape", 
  "query_template": "{url}?debug=false&xvar=production&total_only=false&count=1000&fields=description%2Cpublisher%2Creviewdate%2Ctitle%2Cname%2Cpublicdate%2Cpublisher%2Cdate%2Ccollection%2C%20source&q={query_string}", 
  "query_processors": ["AdaptiveQueryProcessor"], 
  "query_mappings": "",
  "result_processors": [
    "MappingResultProcessor",
    "CosineRelevancyResultProcessor"
  ],
  "response_mappings": "FOUND=count,RESULTS=results",
  "result_mappings": "title=cursor,body=body,date_published=items[0]['publicdate']",
  "results_per_query": 10, 
  "tags": ["InternetArchive", "Search"] 
}
